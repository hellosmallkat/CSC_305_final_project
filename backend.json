{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "authId": {
      "type": "string"
    },
    "birthday": {
      "type": "string"
    },
    "budget": {
      "type": "number"
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "categoryTotals": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "expenses": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "amount": {
            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "store": {
            "type": "string"
          }
        },
        "required": ["amount", "date", "store"]
      }
    },
    "gender": {
      "type": "string"
    },
    "imageURL": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "recurringExpenses": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "amount": {
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "store": {
            "type": "string"
          }
        },
        "required": ["amount", "category", "date", "store"]
      }
    }
  },
  "required": ["authId", "birthday", "budget", "categories", "categoryTotals", "expenses", "gender", "imageURL", "name", "recurringExpenses"]
},{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Adoption": {
      "type": "object",
      "properties": {
        "Daily active Users": {
          "type": "string"
        },
        "Monthly Users": {
          "type": "string"
        },
        "Total number of users": {
          "type": "string"
        }
      },
      "required": ["Daily active Users", "Monthly Users", "Total number of users"]
    },
    "CTR": {
      "type": "object",
      "properties": {
        "usersCompletedGoldenPath": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["usersCompletedGoldenPath"]
    },
    "NPS": {
      "type": "object",
      "properties": {
        "ratings": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      },
      "required": ["ratings"]
    }
  },
  "required": ["Adoption", "CTR", "NPS"]
}
